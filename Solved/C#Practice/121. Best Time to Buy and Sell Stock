public class Solution {
    public int MaxProfit(int[] prices) {
        int buy = prices[0];
        int maxProfit = 0;

        for(int i = 1; i < prices.Length; i++){
            int currentSell = prices[i] - buy;
            if (currentSell > maxProfit){
                maxProfit = currentSell;
            }
            if (prices[i] < buy){
                buy = prices[i];
            }
        }

        return maxProfit;

    }
}

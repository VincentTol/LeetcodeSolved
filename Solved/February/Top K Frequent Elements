class Solution:
    def topKFrequent(self, nums: List[int], k: int) -> List[int]:
        # Create dictionary with key as integer value number times it shows up
        # Dicts not sorted in python unless import so need to use sorted and
        # lambda function to sort them after
        dict1 = {}
        output = []
        for num in nums:
            if num not in dict1:
                dict1[num] = 1
            else:
                dict1[num] += 1
        
        # use lambda to sort output
        sorted_desc = dict(sorted(dict1.items(), key=lambda x: x[1], reverse=True))
        first_k = list(sorted_desc.keys())[:k]

        return first_k
